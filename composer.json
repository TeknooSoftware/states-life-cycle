{
    "name": "unialteri/statesLifeCycle",
    "type": "library",
    "description": "Component to extend the behavior of unialteri/states and provide a service to manage lifes cycles of stated class instance, notify states changes and profile changes",
    "keywords": ["states","class","state pattern","pattern","behavioral software design pattern", "life cycle", "event"],
    "homepage": "http://teknoo.it/states",
    "license": ["MIT","GPL-3.0+"],
    "authors": [
        {
            "name": "Richard DÃ©loge",
            "email": "r.deloge@uni-alteri.com",
            "role":"Software architect, Lead developer"
        }
    ],
    "support": {
        "issues": "https://github.com/UniAlteri/states/issues",
        "source": "https://github.com/UniAlteri/states"
    },
    "require":{
        "php": ">=5.6.0",
        "unialteri/states": "~2.0-beta10",
        "symfony/event-dispatcher":"~2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpunit/phpunit-mock-objects": "~2.3",
        "phpunit/phpunit-selenium": "~1.4",
        "phpunit/phpunit-story": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "UniAlteri\\States\\LifeCycle": ["src/"],
            "UniAlteri\\Tests\\States\\LifeCycle": ["tests/"]
        }
    },
    "minimum-stability": "beta",
    "archive": {
        "exclude": ["demo", "vendor", ".idea"]
    },
    "bin": ["bin/states"],
    "scripts": {
      "test":"phpunit",
      "states":"bin/states"
    }
}
