    "keywords": ["states","class","state pattern","pattern","behavioral software design pattern", "life cycle"],
    "homepage": "http://teknoo.software/states",
    "license": ["MIT"],
    "authors": [
        {
            "name": "Richard DÃ©loge",
            "email": "richarddeloge@gmail.com",
            "role":"Software architect, Lead developer"
        }
    ],
    "support": {
        "issues": "https://github.com/TeknooSoftware/states/issues",
        "source": "https://github.com/TeknooSoftware/states"
    },
    "require":{
        "php": "~7.0",
        "teknoo/states": "~2.0-beta10",
        "symfony/event-dispatcher":"~2.7",
        "symfony/yaml": "~2.7",
        "knplabs/gaufrette": "~0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "5.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Teknoo\\States\\LifeCycle\\": ["src/"],
            "Teknoo\\Tests\\States\\LifeCycle\\": ["tests/"]
        }
